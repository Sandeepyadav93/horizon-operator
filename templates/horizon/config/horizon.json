{
    "command": "/usr/sbin/httpd -DFOREGROUND",
    "config_files": [
        {
            "source": "/var/lib/config-data/httpd.conf",
            "dest": "/etc/httpd/conf/httpd.conf",
            "merge": false,
            "preserve_properties": true,
            "perm": "0644"
        },
        {
            "source": "/run/openstack-dashboard/.secrets/horizon-secret",
            "dest": "/etc/openstack-dashboard/.horizon-secret",
            "merge": false,
            "preserve_properties": true,
            "perm": "0600"
        },
        {
            "source": "/var/lib/config-data/local_settings.py",
            "dest": "/etc/openstack-dashboard/local_settings",
            "merge": false,
            "preserve_properties": true
        },
        {
            "source": "/var/lib/config-data/ssl.conf",
            "dest": "/etc/httpd/conf.d/ssl.conf",
            "merge": false,
            "preserve_properties": true
        },
        {
            "source": "/var/lib/config-data/9999_custom_settings.py",
            "dest": "/etc/openstack-dashboard/local_settings.d/9999_custom_settings.py",
            "merge": false,
            "preserve_properties": true
        }
    ],
    "permissions": [
        {
            "path": "/var/log/horizon",
            "owner": "apache:apache",
            "recurse": true
        },
        {
            "path": "/etc/openstack-dashboard",
            "owner": "apache:apache",
            "recurse": true
        }
    ]
}
